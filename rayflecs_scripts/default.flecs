using rayflecs.module

const blue = Color: {
    a: 255,
    b: 255,
    g: 0,
    r: 0
}

prefab Circle {
  Position: {
    x: 100,
    y: 200
  }
  RayFlecsCircle: {
    diameter: 40
  }
}

for y in 1..11 {
    for x in 1..19 {
        _ : Circle {
            $blue
            Position: {
                x: 100 * x,
                y: 100 * y
            }
        }
    }
}
